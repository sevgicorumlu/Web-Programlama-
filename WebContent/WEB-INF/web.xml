<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>ECommerce</display-name>
  <welcome-file-list>
    <welcome-file>master.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>adminLogin</servlet-name>
    <jsp-file>/index.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>adminLogin</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>index</servlet-name>
    <jsp-file>/index.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>index</servlet-name>
    <url-pattern>/index</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AddBrand</servlet-name>
    <jsp-file>/addBrand.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddBrand</servlet-name>
    <url-pattern>/addBrand.jsp</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>addCategory</servlet-name>
    <jsp-file>/addCategory.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>addCategory</servlet-name>
    <url-pattern>/addCategory</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>addProduct</servlet-name>
    <jsp-file>/addProduct.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>addProduct</servlet-name>
    <url-pattern>/addProduct</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Brand</servlet-name>
    <jsp-file>/brand.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>Brand</servlet-name>
    <url-pattern>/brand</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Category</servlet-name>
    <jsp-file>/category.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>Category</servlet-name>
    <url-pattern>/category</url-pattern>
  </servlet-mapping>
</web-app>